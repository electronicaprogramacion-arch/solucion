@model Reports.Domain.ReportViewModels.Header
@{
    Layout = null;
    string border = "";
    string finalcolspan = "6";
    if (Model.IsAccredited)
    {
        border = "";
    }
}
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=windows-1252">
    <title>@Model.SerialIndReceiv</title>
    <link href=@Url.Content("~/css/Certified.css") rel="stylesheet" />
    <style>
        /* Style Definitions */
        p.MsoNormal,
        li.MsoNormal,
        div.MsoNormal {
        mso-style-unhide: no;
        mso-style-qformat: yes;
        mso-style-parent: "";
        margin-top: 0cm;
        margin-right: 0cm;
        margin-bottom: .15pt;
        margin-left: .5pt;
        text-indent: -.5pt;
        line-height: 107%;
        mso-pagination: widow-orphan;
        font-size: 6.0pt;
        mso-bidi-font-size: 11.0pt;
        font-family: "Arial", sans-serif;
        mso-fareast-font-family: Arial;
        color: black;
        }

        body {
        margin-bottom: 0;
        margin-top: 0;
        }

        .invoice-box {
            max-width: 800px;
            margin: auto;
            padding: 4px;
            font-family: Arial, sans-serif;
            color: black;
            font-size: 8.0pt;
            mso-bidi-font-size: 11.0pt;
            line-height: 107%;
        }

        .invoice-box table {
        width: 100%;
        line-height: inherit;
        text-align: center;
       /*  border-collapse: collapse; */
        border-spacing: unset;
        text-indent: unset;
        }

        .invoice-box table td {
        vertical-align: top;
        font-size: 8.0pt;
        color: black;
        line-height: 107%
        }


        .invoice-box table tr.top table td.title {
        font-size: 45px;
        line-height: 45px;
        color: black;
        }

        .invoice-box table tr.information table td {
        padding-bottom: 10px;
        }

     /*    .invoice-box table tr.heading td {
                    border: 1px solid #ccc;  /* 1px solid black; */
        
        } */

        .table-header {
        text-align: left;
        line-height:0.8;
        }

        td {
        text-align: center;
        padding-top: 1px;
        font-size: 8pt;

        }

        .td-left {
        width: 25%;
        text-align: left;
        font-weight: bold;
        border-collapse: separate;
        border-spacing: 5px;
        padding-left: 5%;
        line-height: 0.5;
        padding-top: 8px;
        padding-bottom: 8px;
        }

        .td-detail {
        width: 25%;
        text-align: left;
        padding-left: 1%;
        border-collapse: separate;
        border-spacing: 5px;
        font-size: 8.0pt;
        line-height: 0.5;
        padding-top: 8px;
        padding-bottom: 8px;
        }

        .td-left1 {
        width: 15%;
        text-align: right;
        font-weight: bold;
        border-collapse: separate;
        border-spacing: 2px;
        }

        .td-detail2 {
        width: 20%;
        text-align: left;
        padding-left: 1%;
        border-collapse: separate;
        border-spacing: 2px;
        }

        .td-title {
        text-align: left;
        padding-left: 5%;
        font-weight: bold;
        font-size: 10.0pt;
        }

        .container {
        display: flex;
        justify-content: space-between;
        align-items: flex-start;
        margin-top: 20px;
        gap: 0px; /* Sin espacio entre las tablas para alineación perfecta */
        }

        tableBody {
        border-collapse: collapse;
        width: 300px; /* Ancho fijo de las tablas */
        }

        th, tdBody {
       border: 1px solid #ccc; 
        text-align: center;

        }

        th {
       
        font-size: 8.0pt;
        }

        tdgrid
        {
        text-align: center;
        padding-top: 1px;
        font-size: 8pt;
        font-weight: bold;
        }

        .vertical-text {
        writing-mode: vertical-rl; 
        transform: rotate(180deg);
        text-align: center;
        }

        .td-leftStrain {
        width: 30%;
        text-align: right;
        font-weight: bold;
        border-collapse: separate;
        border-spacing: 5px;
        }

        /*      NEW */
        body {
            font-family: Arial, sans-serif;
            margin: 0 15px;
        }

        .header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            position: relative;
            padding-bottom: 5px;
        }

        .logo img {
            width: 80%;
            vertical-align: top;
            margin-top: 5px;
        }

        .company-info {
            text-align: right;
            font-size: 12px;
        }

        .title {
            text-align: center;
            font-size: 18px;
            font-weight: bold;
            margin-top: 10px;
        }

        .inspection-info {
            font-style: italic;
            font-weight: bold;
            margin-top: 10px;
        }

        .separator {
            border-bottom: 2px solid black;
            margin-top: 10px; /* Espacio entre INSPECTION INFORMATION y la línea */
        }

        .info-table {
            width: 100%;
            margin-top: 10px;
        }

            .info-table td {
                padding: 5px;
                vertical-align: top;
            }

            .info-table .label {
                font-weight: bold;
            }


            /* TruckScale Advance*/
        .grid-container {
            display: grid;
            grid-template-columns: 1fr 1fr;
            gap: 0%;
        }

        .left-column {
            grid-row: 1 / span 3;
            line-height:0.5px;
        }

        .right-column {
            display: flex;
            flex-direction: column;
            gap: 0px;
            line-height: 0.5px;
            grid-row: 1;
            padding-right: 0%;
        }

        .tableBody {
            width: 100%;
            /* border: 1pt solid black; */
            border-collapse: collapse;
            font-size: 8pt;
        }

            .tableBody th,
            .tableBody td {
               /*  border: 1px solid #ccc; */
                padding: 4px;
                text-align: center;
            }
        /*  Styles for Grids */
        table {
            width: 100%;
            border-collapse: collapse;
            font-family: Arial, sans-serif;
            font-size: 8pt;
            text-align: center;
            border: none;
            line-height: 1;
        }

        .trSign {
            height: 15px;
        }

        .tdSign {
            padding: 5px;
            border: 1px solid #ccc;
        }

        .header {
            font-weight: bold;
            text-decoration: underline;
           
        }

        .left {
            text-align: left;
        }

        .right {
            text-align: right;
        }

        .section-title {
            
            font-weight: bold;
        }
        /*  Styles for footer */
        .tableFooter {
            width: 100%;
            border-collapse: collapse;
            text-align: center;
            font-family: Arial, sans-serif;
            font-size: 8pt;
            line-height: 0.5;
        }

        .trFooter {
            height: 24px;
        }

        .tdFooter {
            border: none;
        }

        .containerFooter {
            max-width: 100%;
            margin: auto;
            font-family: Arial, sans-serif;
            color: black;
            font-size: 8.0pt;
            line-height: 107%;
            text-align: center;
        }


    </style>
</head>
<body lang="ES-CO" style="margin:0 15px;padding:0">
    <div class="title">
        CERTIFICATE OF CALIBRATION
    </div>

    <div class="header">
        <div class="logo">
            <img style="width:80%;top:0px;height:80%" alt=""
                 src="data:image /jpg;base64," />
        </div>
        @if (Model.IsAccredited)
        {
            <div class="logo" style="text-align:right">

                <img style="width:50%;top:0px;height:50%" alt=""
                     src="data:image /jpg;base64," />
            </div>
        }
        else
        {
            <div class="company-info">
                An ISO/IEC 17025 <br /> Accredited Company
            </div>
        }
    </div>

    <div class="inspection-info">
        CALIBRATION INFORMATION
    </div>

    <div class="separator"></div>

    @if (Model.HeaderTruckScaleAdvance != null)
    {
        <div>
            <table>

                <tr>
                    <td class="td-left">CALIBRATION LOCATION: </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.CalibrationLocation</td>
                    <td class="td-left">CALIBRATED FOR: </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.CalibratedFor </td>
                </tr>
                <tr>
                    <td class="td-left">CALIBRATION CERTIFICATE #  </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.CalibrationCertificate</td>
                    <td class="td-left">CUSTOMER ID # </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.CustomerID </td>
                </tr>
                <tr>
                    <td class="td-left">CALIBRATION DATE </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.CalibrationDate</td>
                    <td class="td-left">CALIBRATION DUE DATE </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.CalibrationDueDate </td>
                </tr>
                <tr>
                    <td class="td-left">PLATFORM MANUFACTURER </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.PlatformManufacturer</td>
                    <td class="td-left">INDICATOR MANUFACTURER </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.IndicatorManufacturer </td>
                </tr>
                <tr>
                    <td class="td-left">PLATFORM MODEL</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.PlatformModel</td>
                    <td class="td-left">INDICATOR MODEL</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.IndicatorModel </td>
                </tr>
                <tr>
                    <td class="td-left">PLATFORM SERIAL #</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.PlatformSerial</td>
                    <td class="td-left">INDICATOR SERIAL # T</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.IndicatorSerial </td>
                </tr>
                <tr>
                    <td class="td-left">CAPACITY </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.Capacity</td>
                    <td class="td-left">DIVISION </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.Division </td>
                </tr>
                <tr>
                    <td class="td-left">TYPE OF DECK</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.TypeOfDeck</td>
                    <td class="td-left"># SECTIONS</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.NumberSections </td>
                </tr>
                <tr>
                    <td class="td-left">CLC</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.CLC</td>
                    <td class="td-left">PLATFORM SIZE </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.PlatformSize </td>
                </tr>
                <tr>
                    <td class="td-left">LEAD WIRE SEAL</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.LeadWireSeal</td>
                    <td class="td-left">SCALE DESCRIPTION </td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.ScaleDescription </td>
                </tr>
                <tr>
                    <td class="td-left">ACCESORY EQUIPMENT</td>
                    
                </tr>
                <tr>
                    <td class="td-left">Scoreboard</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.ScoreBoard</td>
                    <td class="td-left">Printer</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.Printer </td>
                </tr>
                <tr>
                    <td class="td-left">Local/Remote</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.LocalRemote</td>
                    <td class="td-left">Other</td>
                    <td class="td-detail">@Model.HeaderTruckScaleAdvance.Other </td>
                </tr>
            </table>
        </div>
    }

    <div style="border: 1px solid #ccc; padding-top:3%">
        <div class="grid-container">
            <div class="left-column">
                @if (@Model.CertifiedWeights.Count > 0)
                {
                    <table>
                        <thead>
                            <tr class="trSign">

                                <th colspan="4">
                                    Certified Weight Build Up Test
                                </th>

                            </tr>
                            <tr class="trSign">
                                <td class="tdSign">
                                    Section
                                </td>
                                <td class="tdSign">
                                    Test Weights
                                </td>
                                <td class="tdSign">
                                    As Found
                                </td>
                                <td class="tdSign">
                                    As Left
                                </td>
                            </tr>

                        </thead>
                        <tbody>

                            @for (int yy = 0; yy < Model.CertifiedWeights.Count; yy++)
                            {
                                <tr class="trSign">
                                    <td class="tdSign">
                                        @Model.CertifiedWeights[yy].Section
                                    </td>
                                    <td class="tdSign">
                                        @Model.CertifiedWeights[yy].TestWeight
                                    </td>

                                    <td class="tdSign">
                                        @Model.CertifiedWeights[yy].AsFound
                                    </td>

                                    <td class="tdSign">
                                        @Model.CertifiedWeights[yy].AsLeft
                                    </td>

                                </tr>
                            }
                        </tbody>

                    </table>
                }
            </div>
            <div class="right-column">
                @if (@Model.SectionsTruckAdvance.Count > 0)
                {
                    <table>

                        <thead>
                            <tr class="trSign">
                                <th colspan="4">
                                    Section Test
                                </th>
                            </tr>
                            <tr class="trSign">
                                <td class="tdSign">
                                    Section
                                </td>
                                <td class="tdSign">
                                    Test Weights
                                </td>
                                <td class="tdSign">
                                    As Found
                                </td>
                                <td class="tdSign">
                                    As Left
                                </td>
                            </tr>


                        </thead>
                        <tbody>

                            @for (int yy = 0; yy < Model.SectionsTruckAdvance?.Count; yy++)
                            {
                                <tr class="trSign">
                                    <td class="tdSign">
                                        @Model.SectionsTruckAdvance[yy].Section
                                    </td>
                                    <td class="tdSign">
                                        @Model.SectionsTruckAdvance[yy].TestWeight
                                    </td>

                                    <td class="tdSign">
                                        @Model.SectionsTruckAdvance[yy].AsFound
                                    </td>

                                    <td class="tdSign">
                                        @Model.SectionsTruckAdvance[yy].AsLeft
                                    </td>

                                </tr>
                            }


                        </tbody>
                    </table>
                }
                @if (@Model.SideToSidesTest.Count > 0)
                {
                    <table>
                        <thead>
                            <tr class="trSign">
                                <th colspan="4">
                                    Side to Side Test/Load Cells
                                </th>
                            </tr>
                            <tr class="trSign">
                                <td class="tdSign">
                                    Section
                                </td>
                                <td class="tdSign">
                                    Test Weights
                                </td>
                                <td class="tdSign">
                                    As Found
                                </td>
                                <td class="tdSign">
                                    As Left
                                </td>
                            </tr>

                        </thead>
                        <tbody>
                            @for (int yy = 0; yy < Model.SideToSidesTest.Count; yy++)
                            {
                                <tr class="trSign">
                                    <td class="tdSign">
                                        @Model.SideToSidesTest[yy].Section
                                    </td>
                                    <td class="tdSign">
                                        @Model.SideToSidesTest[yy].TestWeight
                                    </td>

                                    <td class="tdSign">
                                        @Model.SideToSidesTest[yy].AsFound
                                    </td>

                                    <td class="tdSign">
                                        @Model.SideToSidesTest[yy].AsLeft
                                    </td>

                                </tr>
                            }

                        </tbody>
                    </table>
                }
                @if (@Model.StrainTests.Count > 0)
                {
                    <table >
                        <thead>
                            <tr class="trSign">
                                <th colspan="4">
                                    Strain Test
                                </th>
                            </tr>

                        </thead>
                        <tbody>
                            @for (int yy = 0; yy < Model.StrainTests.Count; yy++)
                            {
                                <tr class="trSign">
                                    <td class="tdSign">
                                        @Model.StrainTests[yy].Name
                                    </td>
                                    <td class="tdSign">
                                        @Model.StrainTests[yy].Value
                                    </td>

                                </tr>
                            }

                        </tbody>
                    </table>
                }
            </div>
        </div>
        <div class="container">
            <table class="tableBody" style="width: 49%; border-left: solid black 1pt; border-right: solid black 1pt; border-top: solid black 1pt; border-bottom: solid black 1pt; padding-top: 2pt;">
                <thead>
                    <tr>
                        <th colspan="@Model.SectionsTruckAdvance?.Count" style="border-bottom:solid 1pt; ">
                            Test As Found
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        @for (int yy = 0; yy < Model.SectionsTruckAdvance?.Count; yy++)
                        {
                            <th>@Model.SectionsTruckAdvance[yy].AsFound</th>
                        }
                    </tr>
                </tbody>

            </table>

            <table class="tableBody" style="width: 49%; border-left: solid black 1pt; border-right: solid black 1pt; border-top: solid black 1pt; border-bottom: solid black 1pt; padding-top: 2pt;">
                <thead>
                    <tr>
                        <th colspan="@Model.SectionsTruckAdvance?.Count" style="border-bottom:solid 1pt;">
                            Test As Left
                        </th>
                    </tr>
                </thead>
                <tbody>
                    <tr>
                        @for (int yy = 0; yy < Model.SectionsTruckAdvance?.Count; yy++)
                        {
                            <th>@Model.SectionsTruckAdvance[yy].AsLeft</th>
                        }
                    </tr>
                </tbody>
            </table>

        </div>
    </div>
    @if (Model.WeightSetList?.Count() > 0)
    {
        <div class="invoice-box">
            <table style="width: 100%; border-left: solid black 1pt; border-right: solid black 1pt; border-top: solid black 1pt; border-bottom: solid black 1pt;">
                <tr>
                    <td colspan="4" style="border-bottom: solid; border-width:1px;">
                        STANDARDS INFORMATION
                    </td>
                </tr>
                <tr>
                    <td style="text-align: center; border: solid black 1.0pt;">
                        EQUIPMENT NUMBER

                    </td>
                    <td style="text-align: center; border:solid black 1.0pt;">
                        SERIAL
                    </td>
                    <td style="text-align: center; border:solid black 1.0pt;">
                        CERTIFICATE NUMBER
                    </td>
                    <td style="text-align: center; border:solid black 1.0pt;">
                        DUE DATE
                    </td>

                </tr>
                @for (int yy = 0; yy < Model.WeightSetList.Count; yy++)
                {
                    <tr style="mso-yfti-irow:3;/* height:96.7pt; */">
                        <td style="text-align: center; border: solid black 1.0pt;">

                            @Model.WeightSetList[yy].PoE
                        </td>
                        <td style="text-align: center; border: solid black 1.0pt;">
                            @Model.WeightSetList[yy].Serial

                        </td>
                        <td style="text-align: center; border: solid black 1.0pt;">
                            @Model.WeightSetList[yy].Ref
                        </td>
                        <td style="text-align: center; border: solid black 1.0pt;">
                            @Model.WeightSetList[yy].CalibrationDueDate
                        </td>

                    </tr>
                }


            </table>
        </div>
    }
    <div class="containerFooter" style="line-height:0.3">
        <div style="margin-top:10px; font-size:6pt">
            <table class="tableFooter" style="border:none; text-align:center; line-height:0.3 ">
                <tr class="trFooter">
                    <td class="tdFooter">
                        <strong>Calibrated by:</strong> @Model.Technician
                    </td>
                    <td class="tdFooter">
                        <strong>Approved by:</strong> @Model.TechnicianAprove
                    </td>
                </tr>

                <tr class="trFooter">
                    <td class="tdFooter" colspan="4" style="font-size:6pt">
                        <strong>Questions?</strong> Call 888-447-2253 or 856-627-0700
                    </td>
                </tr>
                <tr class="trFooter">
                    <td class="tdFooter" colspan="4" style="font-size:7pt">
                        Advance Scale - 2400 Egg Harbor Road, Lindenwold NJ 08021 - Visit:
                        <a href="http://www.advancescale.com" target="_blank">www.advancescale.com</a>
                    </td>
                </tr>

            </table>

        </div>
    </div>
   

</body>
</html>