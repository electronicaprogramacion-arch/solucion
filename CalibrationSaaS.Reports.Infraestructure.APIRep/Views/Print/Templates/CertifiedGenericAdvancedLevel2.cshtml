@model Reports.Domain.ReportViewModels.Header

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=windows-1252">

    <link href=@Url.Content("~/css/Certified.css") rel="stylesheet" />
    <style>
        /* Style Definitions */
        p.MsoNormal,
        li.MsoNormal,
        div.MsoNormal {
        mso-style-unhide: no;
        mso-style-qformat: yes;
        mso-style-parent: "";
        margin-top: 0cm;
        margin-right: 0cm;
        margin-bottom: .15pt;
        margin-left: .5pt;
        text-indent: -.5pt;
        line-height: 107%;
        mso-pagination: widow-orphan;
        font-size: 6.0pt;
        mso-bidi-font-size: 11.0pt;
        font-family: "Arial", sans-serif;
        mso-fareast-font-family: Arial;
        color: black;
        }

        body {
        margin-bottom: 0;
         margin: 20px;
        }

        .invoice-box {
        max-width: 800px;
        margin: auto;
        padding: 4px;
       /*  box-shadow: 0 0 10px rgba(0, 0, 0, .15); */
        font-family: Arial, sans-serif;
        color: black;
        font-size: 8.0pt;
        mso-bidi-font-size: 11.0pt;
        line-height: 107%;

        }

        .invoice-box table {
        width: 100%;
        line-height: inherit;
        text-align: center;
        border-collapse: collapse;
        border-spacing: unset;
        text-indent: unset;
        }

        .invoice-box table td {
        vertical-align: top;
        font-size: 8.0pt;
        color: black;
        line-height: 107%
        }


        .invoice-box table tr.top table td.title {
        font-size: 45px;
        line-height: 45px;
        color: black;
        }

        .invoice-box table tr.information table td {
        padding-bottom: 10px;
        }

        .invoice-box table tr.heading td {
        background: #eee;
        border: 1px solid black;
        font-weight: bold;
        }

        .table-header {
        text-align: left;
        }

        td {
        text-align: center;
        padding-top: 1px;
        font-size: 10.0pt;
        }

        .td-left {
        width: 12%;
        text-align: right;
        font-weight: bold;
        border-collapse: separate;
        border-spacing: 5px;
        }

        .td-detail {
        width: 25%;
        text-align: left;
        padding-left: 1%;
        border-collapse: separate;
        border-spacing: 5px;
        font-size: 10.0pt;
        }

        .td-left1 {
        width: 15%;
        text-align: right;
        font-weight: bold;
        border-collapse: separate;
        border-spacing: 2px;
        }

        .td-detail2 {
        width: 20%;
        text-align: left;
        padding-left: 1%;
        border-collapse: separate;
        border-spacing: 2px;
        }

        .td-title {
        text-align: left;
        padding-left: 5%;
        font-weight: bold;
        font-size: 10.0pt;
        }

        .container {
        display: flex;
        justify-content: space-around;
        margin-top: 20px;
        gap: 5px; /* Espacio entre las tablas */
        }

        tableBody {
        border-collapse: collapse;
        width: 300px; /* Ancho fijo de las tablas */
        }

        th, tdBody {
        border: 1px solid #000;
        padding: 8px;
        text-align: center;

        }

        th {
            background-color: rgba(0, 0, 0, .35);
        font-size: 10.0pt;
        }

        tdgrid
        {
        text-align: center;
        padding-top: 1px;
        font-size: 10.0pt;
        font-weight: bold;
        }

        .vertical-text {
        writing-mode: vertical-rl; 
        transform: rotate(180deg);
        text-align: center;
        }

        .td-leftStrain {
        width: 30%;
        text-align: right;
        font-weight: bold;
        border-collapse: separate;
        border-spacing: 5px;
        }

        .col.center {
            text-align: center;
            font-weight: bold;
        }

        .col.right {
            text-align: right;
            vertical-align: top;
        }

        .col {
            flex: 1;
            padding: 10px;
        }

         .col img {
                max-width: 100%;
                height: auto;
            }

        table {
            width: 100%;
            border-collapse: collapse; /* Asegura que los bordes no se dupliquen */
        }

        th, td {
            border: 1px solid black; /* Agrega borde a todas las celdas */
            padding: 8px;
            text-align: center;
        }

        th {
            background-color: #f2f2f2; 
        }

        /*NEW */

        body {
            font-family: Arial, sans-serif;
            margin: 20px;
        }

        .header {
            display: flex;
            justify-content: space-between;
            align-items: center;
            position: relative;
            padding-bottom: 10px;
        }

        .logo img {
            width: 80%; 
            padding-left: 3%;
        }

        .company-info {
            text-align: right;
            font-size: 12px;
            padding-right: 3%;
        }

        .title {
            text-align: center;
            font-size: 18px;
            font-weight: bold;
            margin-top: 10px;
        }

        .inspection-info {
            font-style: italic;
            font-weight: bold;
            margin-top: 10px;
            padding-left: 3%;
        }

        .separator {
            border-bottom: 2px solid black;
            margin-top: 10px; /* Espacio entre INSPECTION INFORMATION y la línea */
        }

        .info-table {
            width: 100%;
            margin-top: 10px;
        }

         .info-table td {
                padding: 5px;
                vertical-align: top;
            }

        .info-table .label {
                font-weight: bold;
            }

        .tableSign {
            width: 100%;
            border-collapse: collapse;
            text-align: center;
            font-family: Arial, sans-serif;
            font-size: 8pt;
        }

        .trSign {
            height: 24px;
        }

        .tdSign {
            padding: 4px;
            border: none;
        }

        /* Reduce row height for data tables */
        .tableBody tbody tr {
            height: auto;
        }

        .tableBody tbody td {
            padding: 2px 4px;
            line-height: 1.2;
            font-size: 7pt;
            vertical-align: middle;
        }

        .tableBody tbody td[rowspan="3"] {
            vertical-align: middle;
            text-align: center;
        }

        .tableBody thead tr {
            height: auto;
        }

        .tableBody thead td {
            padding: 3px 4px;
            line-height: 1.2;
            font-size: 7pt;
        }

    </style>
</head>
<body lang="ES-CO" style="margin:0;padding:0">
    
    <div class="title">
        INSPECTION REPORT
    </div>

    <div class="header">
        <div class="logo">
            <img style="width:80%;top:0px;height:80%" alt=""
                 src="data:image /jpg;base64," />
        </div>
        <div class="company-info">
            An ISO/IEC 17025 Accredited Company
        </div>
    </div>

    <div class="inspection-info">
        INSPECTION INFORMATION
    </div>

    <div class="separator"></div>
      
    <div class="invoice-box" style="margin-bottom: 0;">
        
    
      
        <p class="MsoNormal"
        style="margin-top:0cm;margin-right:0cm;margin-bottom:3.95pt;margin-left:50.2pt;text-indent:0cm"></p>
        <div>
        </div>

        <table style="border-bottom:none; border-right:none; border-left:none; border-top:none">

            <tbody>
                <tr style="border-bottom:none; border-right:none; border-left:none; border-top:none">
                    <td style="border-bottom:none; border-right:none; border-left:none; border-top:none"> WORK ORDER:</td>
                    <td style="border-bottom:none; border-right:none; border-left:none; border-top:none">@Model.WorkOrderId</td>
                    <td style="border-bottom:none; border-right:none; border-left:none; border-top:none">INSPECTED FOR:</td>
                    <td style="border-bottom:none; border-right:none; border-left:none; border-top:none">@Model.CalibratedFor <br>@Model.Address </td>
                    
                </tr>
  
               
                <tr style="border-bottom:none; border-right:none; border-left:none; border-top:none">
                    <td style="border-bottom:none; border-right:none; border-left:none; border-top:none"> CALIBRATION DATE:</td>
                    <td style="border-bottom:none; border-right:none; border-left:none; border-top:none">@Model.CalibrtionDate</td>
                    <td style="border-bottom:none; border-right:none; border-left:none; border-top:none">NEXT DUE DATE:</td>
                    <td style="border-bottom:none; border-right:none; border-left:none; border-top:none">@Model.CalDueDate</td>
                </tr>

            </tbody>



        </table>

    </div>

   
   
    @if (@Model.AdvanceWods != null)
    {
        <div class="invoice-box">
            <table class="tableBody">
                <thead>
              
                    <tr >
                        <td class="tdgrid" style="font-weight:bold">
                            Scale #
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            Location
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            Make
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                           Model
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            Serial Number
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            Equipment Type
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                           UoM
                        </td>
                        <td class="tdgrid" style="font-weight:bold; width:10%">
                            Test Points
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            0
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            1
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            2
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            3
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            4
                        </td>
                        <td class="tdgrid" style="font-weight:bold">
                            ACT
                        </td>
                     
                    </tr>
                   
                 </thead>
                <tbody>

                    @for (int yy = 0; yy < @Model.AdvanceWods.Count; yy++)
                    {
                        <tr>
                            <td rowspan="3">
                                @Model.AdvanceWods[yy].Scale
                            </td>
                            <td rowspan="3">
                                @Model.AdvanceWods[yy].Location
                            </td>

                            <td rowspan="3">
                                @Model.AdvanceWods[yy].Make
                            </td>

                            <td rowspan="3">
                                @Model.AdvanceWods[yy].Model
                            </td>
                            <td rowspan="3">
                                @Model.AdvanceWods[yy].SerialNumber
                            </td>

                            <td rowspan="3">
                                @Model.AdvanceWods[yy].EquipmentType
                            </td>

                            <td rowspan="3">
                                @Model.AdvanceWods[yy].UOM
                            </td>
                            <td style="width:10%">
                                Applied WT
                            </td>
                            @for (int xx = 0; xx < @Model.AdvanceWods[yy].TestPointsAdvances.Count; xx++)
                            {
                                <td>
                                    @Model.AdvanceWods[yy].TestPointsAdvances[xx].WeightApplied
                                </td>
                            }
                            <td rowspan="3">
                                @Model.AdvanceWods[yy].Act
                            </td>
                        </tr>
                        <tr>
                            <td>
                                As Found
                            </td>
                            @for (int xx = 0; xx < @Model.AdvanceWods[yy].TestPointsAdvances.Count; xx++)
                            {
                                <td>
                                    @Model.AdvanceWods[yy].TestPointsAdvances[xx].AsFound
                                </td>
                            }
                        </tr>
                        <tr>
                            <td>
                                As Left
                            </td>
                            @for (int xx = 0; xx < @Model.AdvanceWods[yy].TestPointsAdvances.Count; xx++)
                            {
                                <td>
                                    @Model.AdvanceWods[yy].TestPointsAdvances[xx].AsLeft
                                </td>
                            }
                        </tr>
                       
                    } 
                </tbody>
               
            </table>

          
            </div>
      }

     <div class="invoice-box">

        <table>
            <thead>

                <tr>
                    <td class="tdgrid" style="font-weight:bold">
                        Scale #
                    </td>
                    <td class="tdgrid" style="font-weight:bold">
                        Comments
                    </td>
                </tr>

            </thead>
            <tbody>
                @if (@Model.GenericStatementsWOD != null)
            {
                @for (int yy = 0; yy < Model.GenericStatementsWOD.Count; yy++)
                {
                    <tr >
                        <td>
                                @Model.GenericStatementsWOD[yy].DataGrid
                        </td>
                            <td>
                                @Model.GenericStatementsWOD[yy].Statement
                            </td>
                    </tr>

                }
            } 
        </tbody>
        </table>
    </div>
     @if (@Model.WeightSetList != null)
    { 
    <div class="invoice-box">
            <div class="inspection-info">
                STANDARDS INFORMATION
            </div>
        <table>
            <thead>

                <tr>
                    <td class="tdgrid" style="font-weight:bold">
                       EQUIPMENT NUMBER
                    </td>
                    <td class="tdgrid" style="font-weight:bold">
                        DESCRIPTION
                    </td>
                    <td class="tdgrid" style="font-weight:bold">
                        CERTIFICATE NUMBER
                    </td>
                    <td class="tdgrid" style="font-weight:bold">
                        DUE DATE
                    </td>
                </tr>

            </thead>
         <tbody>
      
            @for (int yy = 0; yy < Model.WeightSetList.Count; yy++)
            {
                <tr style="mso-yfti-irow:3;/* height:96.7pt; */">
                    <td style="text-align: center; border-left: solid black 1.0pt;">
                        @Model.WeightSetList[yy].PoE
                    </td>
                   
                    <td style="text-align: center; border-left: solid black 1.0pt;">
                        @Model.WeightSetList[yy].Description
                    </td>

                    <td style="text-align: center;">
                        @Model.WeightSetList[yy].Ref
                    </td>

                     <td style="text-align: center;">
                        @Model.WeightSetList[yy].CalibrationDueDate
                    </td>

                </tr>
            }
           
        </tbody>
    </table>
   </div>
}
