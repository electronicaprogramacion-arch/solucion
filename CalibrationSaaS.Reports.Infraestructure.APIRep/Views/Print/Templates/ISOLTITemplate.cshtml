@model CalibrationSaaS.Reports.Infraestructure.APIRep.ViewModels.ForceViewModel

@{
    Layout = null;
}

<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Report LTI</title>

    <style>
        .invoice-box {
            max-width: 800px;
            margin: auto;
            padding: 20px;
            font-size: 12px;
            line-height: 24px;
            font-family: 'Helvetica Neue', 'Helvetica', Helvetica, Arial, sans-serif;
            color: #555;
        }

            .invoice-box table {
                width: 100%;
                line-height: inherit;
                text-align: center;
                border-collapse: collapse;
                    border-spacing:unset;
                    text-indent:unset;
            }

                .invoice-box table td {
                    vertical-align: top;
                }

               
                .invoice-box table tr.top table td.title {
                    font-size: 45px;
                    line-height: 45px;
                    color: #333;
                }

                .invoice-box table tr.information table td {
                    padding-bottom: 30px;
                }

                .invoice-box table tr.heading td {
                    background: #eee;
                    border: 1px solid #0c0c0c;
                    font-weight: bold;
                }
                .table-header{
                    text-align:left;
                }

                 aside{
        float: right;
        width: 40%;
        text-align:center;

    }
     section{
        float: left;
        width: 60%
    }
    </style>
</head>

<body>

    <header>
        <div class="invoice-box">
            <table cellpadding="0" cellspacing="0">
                <tr class="top">
                    <td>
                        <table>
                            <tr>
                                <td style="text-align:left">
                                    <img src="data:image/jpeg;base64," style="width:100%; max-width:150px;" />
                                </td>

                            </tr>
                        </table>
                    </td>
                </tr>
                <tr class="information">
                    <td colspan="7">
                        <table>
                            <tr>
                                <td style="text-align:left">

                                    @Model.Address , @Model.City , @Model.Country<br>
                                    TEL: 90890800 | FAX:4235435 |yyyyy@gmail.com
                                </td>

                                <td style="text-align:right">
                                    <b>LTI Order: @Model.Id</b><br>
                                    <b>Revision: 1 | Rev Date: @Model.CreatedAt

                                </td>
                            </tr>
                        </table>
                    </td>
                </tr>
            </table>
        </div>
    </header>
    <div class="invoice-box">
        <hr color="black">
        <h2>Customer</h2>
        <table>
            <tr class="heading">
                <td>
                    Customer Id
                </td>
                <td>
                    Name
                </td>

                <td>
                    Address
                </td>

            </tr>
            <tr class="item">
                <td>
                    @Model.customer.CustomerID
                </td>
                <td>
                    @Model.customer.Name
                </td>

                <td>
                   @Model.address.StreetAddress1 <br />
                    @Model.address.State <br />
                     @Model.address.Country <br />
                </td>

            </tr>
        </table>
        <hr color="red">
        <h2>Piece Of Equipment</h2>
