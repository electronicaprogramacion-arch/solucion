</html>
<script>
      window.onload = init();

        function init() {
            let div = document.getElementById('universaldiv');
            if (@Model.ShowUniversal=="hidden")
            {
               div.style.visibility = 'hidden';
            
            }
            else
            {
                div.style.visibility = 'visible';
            }
          
            
        }
    </script>